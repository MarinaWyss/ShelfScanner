{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "routes": [
    {
      "src": "/api/saved-books",
      "dest": "/api/saved-books"
    },
    {
      "src": "/api/preferences",
      "dest": "/api/preferences"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 